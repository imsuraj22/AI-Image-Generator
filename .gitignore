server/node_modules/

# Ignore build directories in client
client/build/

# Ignore environment variable files
client/.env
server/.env

# Ignore log files
client/logs/
server/logs/
*.log

# Ignore npm debug files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore operating system files
.DS_Store
Thumbs.db

# Ignore IDE/editor specific files
.vscode/
.idea/
*.swp

